package = io.iohk.atala.prism.swift.cryptoKit
language = Objective-C

headers = SwiftCryptoKit/SwiftCryptoKit-Swift.h
headerFilter = SwiftCryptoKit/*

staticLibraries = libSwiftCryptoKit.a
libraryPaths.ios_x64 = ../iOSLibs/SwiftCryptoKit/build/Release-iphonesimulator/
libraryPaths.ios_arm64 = ../iOSLibs/SwiftCryptoKit/build/Release-iphoneos/
libraryPaths.ios_simulator_arm64 =../iOSLibs/SwiftCryptoKit/build/Release-iphonesimulator/
libraryPaths.macos_x64 = ../iOSLibs/SwiftCryptoKit/build/Release/
libraryPaths.macos_arm64 = ../iOSLibs/SwiftCryptoKit/build/Release/
libraryPaths.tvos_arm64 = ../iOSLibs/SwiftCryptoKit/build/Release-appletvos/
libraryPaths.tvos_simulator_arm64 =../iOSLibs/SwiftCryptoKit/build/Release-appletvsimulator/
libraryPaths.watchos_arm32 = ../iOSLibs/SwiftCryptoKit/build/Release-watchos/
libraryPaths.watchos_arm64 = ../iOSLibs/SwiftCryptoKit/build/Release-watchos/
libraryPaths.watchos_simulator_arm64 = ../iOSLibs/SwiftCryptoKit/build/Release-watchsimulator/
libraryPaths.watchos_x64 = ../iOSLibs/SwiftCryptoKit/build/Release-watchsimulator/

linkerOpts = -L/usr/lib/swift
linkerOpts.ios_x64 = -ios_simulator_version_min 13.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
linkerOpts.ios_arm64 = -iphoneos_version_min 13.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphoneos/
linkerOpts.ios_simulator_arm64 = -ios_simulator_version_min 13.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
linkerOpts.macos_x64 = -macosx_version_min 13.1.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/macosx/
linkerOpts.macos_arm64 = -macosx_version_min 13.1.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/macosx/
linkerOpts.tvos_arm64 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/appletvos/
linkerOpts.tvos_simulator_arm64 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/appletvsimulator/
linkerOpts.watchos_arm32 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/watchos/
linkerOpts.watchos_arm64 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/watchos/
linkerOpts.watchos_simulator_arm64 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/watchsimulator/
linkerOpts.watchos_x64 = -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/watchsimulator/
